{
	"name": "robyn",
	"version": "1.3.0-2",
	"author": "RED Interactive <geeks@ff0000.com>",
	"description": "A highly customizable boilerplate to initialize common projects.",
	"repository": {
		"type": "git",
		"url": "__PROJECT_REPOSITORY__"
	},
	"systemDependencies": {
		"git": "*",
		"grunt": ">=0.3.0"
	},
	"engines": {
		"node": "*"
	},
	"installedPlugins": {},
	"config": {
		"vars": {},
		"dirs": {
			"robyn": ".robyn",
			"tasks": "robyn/tasks",
			"config": "robyn/config"
		},
		"requiredPaths": [
			".robyn",
			"node_modules"
		],
		"excludedPaths": [
			".git",
			".robyn",
			"node_modules"
		],
		"ignoreTasks": [
			"concat",
			"init",
			"lint",
			"min",
			"qunit",
			"test"
		]
	}
}
